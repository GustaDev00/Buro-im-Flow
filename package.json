{
  "name": "buro-im-flow",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "imgs": "NODE_ENV=development node ./script/post-imgs.js",
    "img-donwload:prod": "NODE_ENV=production node ./script/post-imgs.js",
    "zip": "zip -r build.zip out",
    "convert-medias": "node ./script/convert-to-webp.js",
    "convert-video": "node ./script/process-video.js",
    "atoms": "node ./script/folder.js atoms",
    "molecules": "node ./script/folder.js molecules",
    "organisms": "node ./script/folder.js organisms",
    "svg": "node ./script/create-svg-icon.js",
    "page": "node ./script/create-page.js",
    "dev": "yarn imgs && node ./script/dev.js",
    "build": "yarn img-donwload:prod && yarn convert-medias && yarn convert-video && next build",
    "test": "next build",
    "publish": "yarn convert-medias && yarn convert-video && next build && yarn zip",
    "analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@studio-freight/lenis": "^1.0.42",
    "@next/bundle-analyzer": "^15.1.4",
    "@next/third-parties": "^15.1.4",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "ffmpeg-static": "^5.2.0",
    "formik": "^2.4.6",
    "global": "^4.4.0",
    "gsap": "^3.12.5",
    "inquirer": "^12.3.1",
    "next": "15.0.1",
    "prompts": "^2.4.2",
    "react": "19.0.0-rc-69d4b800-20241021",
    "react-dom": "19.0.0-rc-69d4b800-20241021",
    "react-icons": "^5.4.0",
    "react-responsive": "^10.0.0",
    "styled-components": "^6.1.13",
    "split-type": "^0.3.4",
    "yarn": "^1.22.22",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.10.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "15.0.1",
    "typescript": "^5"
  }
}
